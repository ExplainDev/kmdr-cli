{
  "name": "kmdr.sh",
  "version": "0.0.4",
  "description": "A command-line interface for explaining commands in the terminal",
  "main": "dist/src/index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest --verbose",
    "start": "node dist/index.js"
  },
  "bin": {
    "kmdr": "dist/src/bin.js"
  },
  "keywords": [
    "shell",
    "bash",
    "terminal"
  ],
  "repository": "git@github.com:kommandr/kmdr.sh.git",
  "author": "Eddie Ramirez <djedi.r@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/chalk": "^2.2.0",
    "@types/commander": "^2.12.2",
    "@types/inquirer": "^6.0.3",
    "@types/node": "^12.0.2",
    "@types/node-emoji": "^1.8.1",
    "@types/ora": "^3.2.0",
    "jest": "^24.8.0",
    "prettier": "^1.17.1",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.4.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "dist/",
      "tests/data/"
    ],
    "testRegex": "tests/.*\\.(ts|tsx|js)$"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "boxen": "^4.1.0",
    "chalk": "^2.4.2",
    "commander": "^2.20.0",
    "inquirer": "^6.3.1",
    "js-yaml": "^3.13.1",
    "log-symbols": "^3.0.0",
    "node-emoji": "^1.10.0",
    "ora": "^3.4.0"
  }
}
