{
  "name": "kmdr",
  "version": "0.4.0",
  "description": "A command-line interface for explaining commands in the terminal",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest --verbose",
    "start:prod": " NODE_ENV=production node dist/index.js",
    "start:dev": "KMDR_API_URL=http://localhost:8081/api/graphql NODE_ENV=development node dist/index.js"
  },
  "bin": {
    "kmdr": "dist/bin.js"
  },
  "keywords": [
    "shell",
    "bash",
    "terminal"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ediardo/kmdr.sh.git"
  },
  "author": "Eddie Ramirez <djedi.r@gmail.com> (https://github.com/ediardo)",
  "homepage": "https://kmdr.sh",
  "license": "MIT",
  "devDependencies": {
    "@types/inquirer": "^6.0.3",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.7.8",
    "@types/node-emoji": "^1.8.1",
    "@types/tunnel": "0.0.1",
    "@types/uuid": "^3.4.5",
    "@types/word-wrap": "^1.2.1",
    "jest": "^24.8.0",
    "kmdr-parser": "^1.1.4",
    "prettier": "^1.17.1",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.4.5"
  },
  "contributors": [
    {
      "name": "Ianeta Hutchinson",
      "email": "ianeta.hutch@gmail.com"
    }
  ],
  "jest": {
    "testPathIgnorePatterns": [
      "dist/",
      "build/",
      "src/",
      "node_modules/"
    ],
    "testEnvironment": "node",
    "testRegex": "tests/.*\\.(ts|tsx|js)$",
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "diagnostics": {
          "ignoreCodes": [
            2345
          ]
        }
      }
    }
  },
  "dependencies": {
    "axios": "^0.18.0",
    "chalk": "^2.4.2",
    "commander": "^2.20.0",
    "inquirer": "^7.0.0",
    "kmdr-ast": "^2.0.1",
    "ora": "^3.4.0",
    "tunnel": "^0.0.6",
    "word-wrap": "^1.2.3"
  }
}
